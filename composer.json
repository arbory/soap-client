{
    "name": "arbory/soap-client",
    "type": "library",
    "description": "Easily extendable SOAP client with WS-Addressing support",
    "keywords": ["soap", "wsa", "ws-addressing"],
    "minimum-stability": "stable",
    "homepage": "http://www.prezent.nl",
    "license": "MIT",
    "authors": [
        {
            "name": "Sander Marechal",
            "email": "sander@prezent.nl"
        },
        {
            "name": "Prezent Internet B.V.",
            "email": "github@prezent.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-soap": "*",
        "ramsey/uuid": "^3.0|^4.0",
        "symfony/event-dispatcher": "^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": { "Prezent\\Soap\\Client\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
